{
  "name": "proyecto-inventario",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "src/electron/main.js",
  "scripts": {
    "dev": "vite",
    "dev:electron": "electron .",
    "back":"cd Backend && .venv/Scripts/python -m uvicorn app.main:app --reload --port 4000",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "dist:mac": "npm run dev:electron && npm run build && electron-builder --mac --arm64",
